{
  "openapi": "3.0.3",
  "info": {
    "title": "Swagger Petstore - OpenAPI 3.0",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "https://application-f3.zsxpn1u729m.us-south.codeengine.appdomain.cloud"
    }
  ],
  "paths": {
    "/status": {
      "get": {
        "operationId": "getStatus",
        "responses": {
          "200": {
            "description": "A JSON object",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/golfstock": {
      "post": {
        "operationId": "postgolfstock",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "club": {
                    "type": "string"
                  },
                  "brand": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "A golf inventory object.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "identifiers": {
                      "type": "object",
                      "properties": {
                        "brand": {
                          "type": "string",
                          "example": "nike"
                        },
                        "club": {
                          "type": "string",
                          "example": "putter"
                        }
                      }
                    },
                    "instock": {
                      "type": "boolean",
                      "example": true
                    },
                    "number": {
                      "type": "integer",
                      "example": 94
                    },
                    "cost": {
                      "type": "number",
                      "example": 10.32
                    },
                    "hours": {
                      "type": "number",
                      "example": 3
                    },
                    "key": {
                      "type": "string",
                      "example": "no_match"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/order": {
      "post": {
        "operationId": "postOrder",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "club": {
                    "type": "string"
                  },
                  "brand": {
                    "type": "string"
                  },
                  "number": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Order Confirmation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "total": {
                      "type": "number"
                    },
                    "message": {
                      "type": "string",
                      "example": "Thank you for your order!"
                    },
                    "key": {
                      "type": "string",
                      "example": "no_match"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/sell": {
      "post": {
        "operationId": "postSell",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "club": {
                    "type": "string"
                  },
                  "brand": {
                    "type": "string"
                  },
                  "number": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Selling Confirmation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "total": {
                      "type": "number"
                    },
                    "message": {
                      "type": "string",
                      "example": "Thank you for selling us flowers!"
                    },
                    "key": {
                      "type": "string",
                      "example": "no_match"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
